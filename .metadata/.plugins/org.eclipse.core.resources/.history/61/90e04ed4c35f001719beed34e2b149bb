package testclass;

public class BlockingQueueCommunication {

	public static void main(String[] args) {
		final Business bus = new Business();
		new Thread(new Runnable() {
			@Override
			public void run() {
				for (int i = 0; i < 50; i++) {
					bus.sub(i);
				}
			}
		}).start();
		for (int i = 0; i < 50; i++) {
			bus.main(i);
		}
	}

}
